{
  "name": "eskoolportal",
  "version": "1.0.0",
  "description": "School students management applications",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node server.js",
    "server": "nodemon server.js --ignore client/",
    "client": "npm start --prefix client",
    "develop": "concurrently \"npm run server\" \"npm run client\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client && NPM_CONFIG_PRODUCTION=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sawbeanraz/eskoolportal.git"
  },
  "author": "Sabin Raj DANGOL",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sawbeanraz/eskoolportal/issues"
  },
  "homepage": "https://github.com/sawbeanraz/eskoolportal#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^3.5.1",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.2.2",
    "lodash": "^4.17.10",
    "mongoose": "^5.1.3",
    "npm": "^6.1.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pdfkit": "^0.8.3",
    "validator": "^10.2.0",
    "voilab-pdf-table": "^0.4.0"
  },
  "devDependencies": {
    "lerna": "^3.16.4",
    "nodemon": "^1.17.5"
  }
}
